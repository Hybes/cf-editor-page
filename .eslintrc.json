{
	"root": true,
	"env": {
		"browser": true,
		"node": true
	},
	"extends": ["@nuxt/eslint-config", "plugin:prettier/recommended"],
	"rules": {
		"vue/multi-word-component-names": "off",
		"vue/no-multiple-template-root": "off",
		"no-console": "off",
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		]
	}
}
